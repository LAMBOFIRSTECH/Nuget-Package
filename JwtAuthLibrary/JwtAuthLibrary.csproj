<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>1.0.1</Version>
    <Authors>artur lambo</Authors>
    <Company>LamboFT</Company>
    <Description>Bibliothèque pour interagir avec HashiCorp Vault et Récupérer la clé publique du token JWT</Description>
    <PackageTags>JWT, HashiCorp, Vault, RSA</PackageTags>
    <RepositoryUrl>https://gitlab.com/artur437810/nugget-management-package/JwtAuthLibrary</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="9.0.0" />
    <PackageReference Include="Microsoft.IdentityModel.JsonWebTokens" Version="7.7.1" />
    <PackageReference Include="VaultSharp" Version="1.17.5.1" />
  </ItemGroup>

</Project>
